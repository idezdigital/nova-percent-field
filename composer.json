{
    "name": "idez/nova-percent-field",
    "description": "A Laravel Nova percent field.",
    "keywords": [
        "laravel",
        "nova",
        "percent",
        "field",
        "idez"
    ],
    "authors": [
        {
            "name": "Rafael Laurindo",
            "email": "rafael@idez.com.br",
            "homepage": "https://idez.com.br",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/idezdigital/nova-percent-field/issues"
    },
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0|^8.1"
    },
    "autoload": {
        "psr-4": {
            "Idez\\NovaPercentField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Idez\\NovaPercentField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
